az group create --name MY-ResourceGroup --location westeurope

az vm create --name MY-VM --resource-group MY-ResourceGroup --image Ubuntu2204 --size Standard_D2s_v5 --public-ip-sku Standard --admin-username mrunal --authentication-type ssh --generate-ssh-keys

//create NSG
az network nsg create \
  --resource-group MY-ResourceGroup \
  --name myNSG \
  --location westeurope
  
# Add SSH rule to allow inbound traffic on port 22
az network nsg rule create \
  --resource-group MY-ResourceGroup \
  --nsg-name myNSG \
  --name AllowSSH \
  --protocol Tcp \
  --direction Inbound \
  --priority 1000 \
  --source-address-prefixes '*' \
  --source-port-ranges '*' \
  --destination-address-prefixes '*' \
  --destination-port-ranges 22 \
  --access Allow
  
# Create VNet with Subnet
az network vnet create \
  --resource-group MY-ResourceGroup \
  --name M-VNET \
  --address-prefix "10.0.0.0/16" \
  --subnet-name M-SNET \
  --subnet-prefix "10.0.1.0/24" \
  --location westeurope
  




























  
